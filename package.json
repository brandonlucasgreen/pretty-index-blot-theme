{
  "name": "pretty-index",
  "locals": {
    "max_width": 700,
    "page_size": "15",
    "text_color": "#000000",
    "background_color": "#FFFFFF",
    "navigation_background_color": "#FFFFFF",
    "accent_color": "#200220",
    "hide_dates": false,
    "date_display": "Y-MM-DD",
    "font": {
      "name": "Golos Text",
      "id": "golos-text",
      "stack": "'Golos Text'",
      "tags": [
        "sans"
      ],
      "line_height": "1.5",
      "line_width": 38,
      "font_size": "14",
      "styles": "@font-face{font-family:'Golos Text';font-style:normal;font-weight:400;src:url('https://cdn.blot.im/fonts/golos-text/400.eot?version=57fa76&extension=.eot');src:url('https://cdn.blot.im/fonts/golos-text/400.eot?version=57fa76&extension=.eot#iefix') format('embedded-opentype'),url('https://cdn.blot.im/fonts/golos-text/400.woff?version=f6d3e9&extension=.woff') format('woff'),url('https://cdn.blot.im/fonts/golos-text/400.ttf?version=95a64c&extension=.ttf') format('truetype'),url('https://cdn.blot.im/fonts/golos-text/400.otf?version=72e2c0&extension=.otf') format('opentype')}.pull-double{margin-left:-.39em}.push-double{margin-right:.39em}.pull-single{margin-left:-.22em}.push-single{margin-right:.22em}.push-double,.push-single{display:inline-block}"
    },
    "syntax_highlighter": {
      "id": "stackoverflow-light",
      "styles": ".hljs{color:#2f3337;background:#f6f6f6}.hljs-subst{color:#2f3337}.hljs-comment{color:#656e77}.hljs-attr,.hljs-doctag,.hljs-keyword,.hljs-meta .hljs-keyword,.hljs-section,.hljs-selector-tag{color:#015692}.hljs-attribute{color:#803378}.hljs-name,.hljs-number,.hljs-quote,.hljs-selector-id,.hljs-template-tag,.hljs-type{color:#b75501}.hljs-selector-class{color:#015692}.hljs-link,.hljs-regexp,.hljs-selector-attr,.hljs-string,.hljs-symbol,.hljs-template-variable,.hljs-variable{color:#54790d}.hljs-meta,.hljs-selector-pseudo{color:#015692}.hljs-built_in,.hljs-literal,.hljs-title{color:#b75501}.hljs-bullet,.hljs-code{color:#535a60}.hljs-meta .hljs-string{color:#54790d}.hljs-deletion{color:#c02d2e}.hljs-addition{color:#2f6f44}.hljs-emphasis{font-style:italic}.hljs-strong{font-weight:700}pre code.hljs{display:block;overflow:auto;padding:1em;margin:1em 0;white-space:pre-wrap;white-space:-moz-pre-wrap;white-space:-pre-wrap;white-space:-o-pre-wrap;word-wrap:break-word}.hljs,code.hljs{padding:0;margin:0}"
    },
    "syntax_highlighter_font": {
      "name": "System mono",
      "id": "system-mono",
      "stack": "Consolas, monaco, monospace",
      "tags": [
        "mono"
      ],
      "line_height": 1.6,
      "line_width": 39,
      "font_size": "16",
      "styles": ""
    },
    "index_layout": "titles",
    "index_layout_options": [
      "titles",
      "title_and_summary",
      "post_in_full"
    ],
    "navigation_location": "top",
    "navigation_location_options": [
      "top",
      "left",
      "right"
    ],
    "navigation_alignment": "justified",
    "navigation_alignment_options": [
      "justified",
      "left",
      "right",
      "center"
    ],
    "site_alignment": "center",
    "site_alignment_options": [
      "center",
      "left",
      "right"
    ],
    "demo_folder": "david",
    "template_categories": [
      "writing"
    ]
  },
  "views": {
    "search.html": {
      "partials": {
        "title": "Search - {{{title}}}",
        "description": "Search {{{title}}}"
      }
    },
    "archives.html": {
      "url": "/archives",
      "partials": {
        "title": "archive - {{{title}}}",
        "description": "All the entries posted on {{{title}}}"
      }
    },
    "entries.html": {
      "partials": {
        "title": "{{{title}}}",
        "description": "The latest on {{{title}}}"
      }
    },
    "entry.html": {
      "partials": {
        "title": "{{{entry.title}}} - {{{title}}}",
        "description": "{{{entry.summary}}}"
      }
    },
    "error.html": {
      "partials": {
        "title": "{{{error.title}}} - {{{title}}}",
        "description": "Error on {{{title}}}"
      }
    },
    "tagged.html": {
      "partials": {
        "title": "{{{tag}}} - {{{title}}}",
        "description": "All of the entries posted on {{{title}}} tagged {{{tag}}}"
      }
    }
  }
}